{
  "name": "mb-x/architect-bundle",
  "type": "symfony-bundle",
  "description": "This Bundle provides an architecture to separate the different job layers in your Symfony application.",
  "keywords": ["Symfony2","Symfony3", "design pattern","Utils", "Manager", "Form Handler"],
  "homepage": "https://github.com/mb-x/architect-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Mohammed Bengrich",
      "email": "mbengrich.dev@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "symfony/framework-bundle": ">=2.7",
    "twig/twig": "*",
    "doctrine/doctrine-bundle": "*"
  },
  "autoload": {
    "psr-4": {
      "Mbx\\ArchitectBundle\\": ""
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Mbx\\ArchitectBundle\\Tests\\": "Tests/"
    }
  }
}