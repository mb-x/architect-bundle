{
  "name": "mb-x/symfony-bootstrap-bundle",
  "type": "symfony-bundle",
  "description": "Symfony MbxSymfonyBootstrapBundle",
  "keywords": ["Utils"],
  "homepage": "https://github.com/mb-x/symfony-bootstrap-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Mohammed Bengrich",
      "email": "mbengrich.dev@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.5.0",
    "symfony/form": "^2.7 || ^3.0",
    "symfony/framework-bundle": "^2.7 || ^3.0",
    "symfony/twig-bundle": "^2.7 || ^3.0",
    "doctrine/doctrine-bundle" : "*"
  },
  "require-dev": {
    "doctrine/doctrine-bundle": "^1.3",
    "symfony/console": "^2.7 || ^3.0",
    "symfony/phpunit-bridge": "^2.7 || ^3.0",
    "symfony/yaml": "^2.7 || ^3.0",
    "phpunit/phpunit": "~4.8|~5.0"
  },
  "conflict": {
    "symfony/doctrine-bridge": "<2.7"
  },
  "autoload": {
    "psr-4": {
      "Mbx\\SymfonyBootstrapBundle\\": ""
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Mbx\\SymfonyBootstrapBundle\\Tests\\": "Tests/"
    }
  }
}